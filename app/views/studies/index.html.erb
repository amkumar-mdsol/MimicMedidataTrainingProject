<h1>Studies <%=link_to 'Add Study', new_study_path %> </h1>
<a></a>
<div style="border: 1px solid;">
  <table>
    <tbody>
      <tr>
        <th> Name </th>
        <th> Age Limit </th>
        <th> Drug </th>
        <th> Phase </th>
        <th> Symptoms </th>
        <th> Image </th>
      </tr>
      <% cache @studies do %>
        <% @studies.each do |study| %>
          <%= render partial: 'study', locals: { study: study } %>
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>